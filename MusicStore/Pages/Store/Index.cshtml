@page
@model MusicStore.Pages.Store.IndexModel
@{
    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>
<p>
    Select from @Model.Genres.Count() genres:
</p>
<ul class="list-group">
    @foreach (var genre in Model.Genres)
    {
        <li class="list-group-item"><a asp-page="Browse" asp-route-genre="@genre.Name">@genre.Name</a></li>
    }
</ul>